<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="eu.greenhouseinnovation.selly.common.model.dao.CategoryDao">

    <select id="getCategories" resultType="Category" resultMap="CategoryResult">
        SELECT * FROM category;
    </select>
    
    <resultMap type="Category" id="CategoryResult">
        <constructor/>

        <result property="sId" column="id"/>
        <result property="sCategoryTitle" column="title"/>
    </resultMap>
    
</mapper>