<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE logback>
<configuration scan="true" scanPeriod="60 seconds">
    <!-- Configuration -->
    <contextName>api</contextName>
    <include resource="logback-appenders.xml"/>
    <root level="info">
        <appender-ref ref="console"/>
        <appender-ref ref="file"/>
    </root>
</configuration>
